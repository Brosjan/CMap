	<Frame name="CMinimapOptionFrame" frameStrata="LOW" hidden="true" enableMouse="true" parent="UIParent">
		<Size>
			<AbsDimension x="700" y="200" />
		</Size>
		<Anchors>
			<Anchor point="BOTTOM" relativeTo="UIParent" relativePoint="CENTER">
				<Offset>
